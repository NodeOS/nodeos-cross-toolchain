{
  "name": "nodeos-cross-toolchain",
  "version": "0.0.1",
  "description": "Cross-toolchain to build NodeOS based on Compressed Linux From Scratch Embedded",
  "main": "./out",
  "bin": {
    "ar": "bin/ar",
    "as": "bin/as",
    "c++": "bin/c++",
    "g++": "bin/g++",
    "gcc": "bin/gcc",
    "ld": "bin/ld",
    "ld.bfd": "bin/ld.bfd",
    "nm": "bin/nm",
    "objcopy": "bin/objcopy",
    "objdump": "bin/objdump",
    "ranlib": "bin/ranlib",
    "strip": "bin/strip"
  },
  "scripts": {
    "install": "scripts/preinstall && scripts/build",
    "postinstall": "scripts/postinstall",
    "test": "scripts/test",
    "unbuild": "scripts/unbuild"
  },
  "keywords": [
    "NodeOS",
    "CLFS",
    "embedded",
    "toolchain",
    "cross"
  ],
  "author": "Jesús Leganés Combarro \"piranna\" <piranna@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "download-manager": "^0.0.15",
    "fs-extra": "^0.26.5"
  }
}
